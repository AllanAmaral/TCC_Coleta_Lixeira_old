<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Business.Objects.Motorista" table="motorista">
    <id name="idMotorista" type="java.lang.Integer">
      <column name="ID_MOTORISTA"/>
      <generator class="identity"/>
    </id>
    <property name="nomeMotorista" type="string">
      <column length="255" name="NOME_MOTORISTA" not-null="true"/>
    </property>
  </class>
  
  <class name="Business.Objects.CaminhaoMotorista" table="caminhao_motorista">
    <id name="idCaminhaoMotorista" type="java.lang.Integer">
      <column name="ID_CAMINHAO_MOTORISTA"/>
      <generator class="identity"/>
    </id>
    <property name="idCaminhao" type="java.lang.Integer">
      <column name="ID_CAMINHAO" not-null="true"/>
    </property>
    <property name="idMotorista" type="java.lang.Integer">
      <column name="ID_MOTORISTA" not-null="true"/>
    </property>
    <property name="dataHora" type="timestamp">
      <column length="19" name="DATA_HORA" not-null="true"/>
    </property>
  </class>
  
  <class name="Business.Objects.HistoricoColeta" table="historico_coleta">
  </class>
  
  <class name="Business.Objects.Lixeira" table="lixeira">
    <id name="idLixeira" type="java.lang.Integer">
      <column name="ID_LIXEIRA"/>
      <generator class="identity"/>
    </id>
    <property name="capacidadeLixeiraKg" type="java.math.BigDecimal">
      <column name="CAPACIDADE_LIXEIRA_KG" not-null="true"/>
    </property>
    <property name="capacidadeLixeiraLt" type="java.math.BigDecimal">
      <column name="CAPACIDADE_LIXEIRA_LT" not-null="true"/>
    </property>
    <property name="coletadoLixeiraKg" type="java.math.BigDecimal">
      <column name="COLETADO_LIXEIRA_KG"/>
    </property>
    <property name="coletadoLixeiraLt" type="java.math.BigDecimal">
      <column name="COLETADO_LIXEIRA_LT"/>
    </property>
    <property name="latitude" type="java.math.BigDecimal">
      <column name="LATITUDE" not-null="true"/>
    </property>
    <property name="longitude" type="java.math.BigDecimal">
      <column name="LONGITUDE" not-null="true"/>
    </property>
  </class>
  
  <class name="Business.Objects.Caminhao" table="caminhao">
    <id name="idCaminhao" type="java.lang.Integer">
      <column name="ID_CAMINHAO"/>
      <generator class="identity"/>
    </id>
    <property name="capacidadeCaminhaoKg" type="java.math.BigDecimal">
      <column name="CAPACIDADE_CAMINHAO_KG" not-null="true"/>
    </property>
    <property name="capacidadeCaminhaoLt" type="java.math.BigDecimal">
      <column name="CAPACIDADE_CAMINHAO_LT" not-null="true"/>
    </property>
    <property name="coletadoCaminhaoKg" type="java.math.BigDecimal">
      <column name="COLETADO_CAMINHAO_KG"/>
    </property>
    <property name="coletadoCaminhaoLt" type="java.math.BigDecimal">
      <column name="COLETADO_CAMINHAO_LT"/>
    </property>
  </class>
</hibernate-mapping>
